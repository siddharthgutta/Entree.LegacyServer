{
  "extends": "airbnb",
  "plugins": [
    "react",
    "mocha"
  ],
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "rules": {
    "mocha/no-exclusive-tests": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "object-curly-spacing": [
      2,
      "never"
    ],
    "object-shorthand": [
      2,
      "always"
    ],
    "no-console": 0,
    "no-extra-parens": 2,
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "valid-jsdoc": 2,
    "no-unreachable": 2,
    "prefer-template": 2
  },
  "ecmaFeatures": {
    "objectLiteralShorthandProperties": true,
    "spread": true
  }
}