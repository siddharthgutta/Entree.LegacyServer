{
  "start": "node index.compiled.js",
  "hooksDir": "hooks/",
  "preferPort": {
    "mode": {
      "manual": 3000
    },
    "branch": {
      "socket-shared": 2996,
      "socket-test": 2997,
      "socket-master": 2998
    }
  },
  "env": {
    "mode": {
      "manual": {
        "NODE_ENV": "staging",
        "HTTPS_REJECT_UNAUTHORIZED": false,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0,
        "HTTPS_SSL_KEY": "./keys/localhost.key",
        "HTTPS_SSL_CERTIFICATE": "./keys/localhost.crt",
        "HTTPS_SSL_CA": "./keys/localhost.crt"
      },
      "stage": {
        "NODE_ENV": "staging",
        "HTTPS_REJECT_UNAUTHORIZED": false,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0
      },
      "release": {
        "NODE_ENV": "production",
        "HTTPS_REJECT_UNAUTHORIZED": true,
        "NODE_TLS_REJECT_UNAUTHORIZED": true
      }
    },
    "branch": {
      "socket-shared": {
        "IPC_ALLOW_REMOTE": true
      },
      "socket-test": {
        "IPC_ALLOW_REMOTE": false
      },
      "socket-master": {
        "IPC_ALLOW_REMOTE": false,
        "HTTPS_SSL_KEY": "./keys/textentree.com.key",
        "HTTPS_SSL_CERTIFICATE": "./keys/textentree.com.crt",
        "HTTPS_SSL_CA": "./keys/textentree.com.chain.crt"
      }
    }
  },
  "pm2": {
    "restart_delay": 10000,
    "watch": false,
    "min_uptime": "20s",
    "max_restarts": 3,
    "script": "./index.compiled.js",
    "error_file": "/dev/null",
    "out_file": "/dev/null",
    "next_gen_js": false,
    "max_memory_restart": "4G"
  }
}