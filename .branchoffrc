{
  "start": "node index.compiled.js",
  "hooksDir": "hooks/",
  "preferPort": {
    "mode": {
      "manual": 3000
    },
    "branch": {
      "socket-shared": 2996
    }
  },
  "env": {
    "default": {
      "SOCKET_SERVER_PROTOCOL": "http",
      "SOCKET_SERVER_HOSTNAME": "127.0.0.1"
    },
    "mode": {
      "default": {
        "SOCKET_SERVER_PORT": 2996,
        "SOCKET_SERVER_PATH": "",
        "SOCKET_SERVER_SEARCH": ""
      },
      "manual": {
        "NODE_ENV": "staging",
        "HTTPS_REJECT_UNAUTHORIZED": false,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0,
        "HTTPS_SSL_KEY": "./keys/localhost.key",
        "HTTPS_SSL_CERTIFICATE": "./keys/localhost.crt",
        "HTTPS_SSL_CA": "./keys/localhost.crt",
        "SOCKET_SERVER_HOSTNAME": "0.0.0.0",
        "SOCKET_SERVER_PORT": 2996
      },
      "stage": {
        "NODE_ENV": "staging",
        "HTTPS_REJECT_UNAUTHORIZED": false,
        "NODE_TLS_REJECT_UNAUTHORIZED": 0
      },
      "release": {
        "NODE_ENV": "production",
        "HTTPS_REJECT_UNAUTHORIZED": true,
        "NODE_TLS_REJECT_UNAUTHORIZED": true
      }
    },
    "branch": {
      "socket-shared": {
        "IPC_ALLOW_REMOTE": true
      }
    }
  },
  "pm2": {
    "restart_delay": 10000,
    "watch": false,
    "min_uptime": "20s",
    "max_restarts": 3,
    "script": "./index.compiled.js",
    "error_file": "/dev/null",
    "out_file": "/dev/null",
    "next_gen_js": false,
    "max_memory_restart": "4G"
  }
}