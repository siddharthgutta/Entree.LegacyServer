{
  "env":{
    "client":{
      "presets":[
        "es2015",
        "react",
        "stage-0"
      ],
      "plugins":[
        "array-includes",
        "syntax-async-functions",
        "transform-object-rest-spread",
        "syntax-class-properties",
        [
          "transform-async-to-module-method",
          {
            "module":"bluebird",
            "method":"coroutine"
          }
        ]
      ]
    },
    "server":{
      "presets":[
        "node5",
        "stage-0"
      ],
      "plugins":[
        "array-includes",
        "transform-runtime",
        "transform-object-rest-spread",
        "syntax-class-properties",
        [
          "transform-async-to-module-method",
          {
            "module":"bluebird",
            "method":"coroutine"
          }
        ]
      ]
    }
  }
}