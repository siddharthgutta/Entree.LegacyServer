  "pre-commit": [
    "compile",
    "branch-off-test",
    "lint"
  ],
  "homepage": "https://github.com/siddharthgutta/Entree.Server#readme",
  "devDependencies": {
    "babel-eslint": "4.1.8",
    "cordova": "6.0.0",
