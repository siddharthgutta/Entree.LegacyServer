    "eslint-plugin-markdown": "1.0.0-beta.1",
    "eslint-plugin-mocha": "1.1.0",
    "eslint-plugin-babel": "3.1.0",
    "eslint-plugin-react": "3.16.1",
    "grunt-cli": "0.1.13",
    "mocha": "2.3.4",
    "pre-commit": "1.1.2"
  }
