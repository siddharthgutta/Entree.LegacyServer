    "babelify": "https://github.com/bluejamesbond/babelify",
    "bluebird": "3.2.1",
    "body-parser": "1.15.0",
    "callsite": "1.0.0",
    "config": "1.19.0",
    "convert-source-map": "1.1.3",
    "exit-hook": "1.1.1",
