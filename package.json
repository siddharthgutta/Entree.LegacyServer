  "scripts": {
    "test": "set -e; mocha --timeout 15000 --no-exit -R nyan",
    "tests": "set -e; for f in ./tests/*-test.compiled.js ; do npm run test $f; done",
    "start": "node index.compiled.js",
    "test-ci": "set -e; mocha --timeout 15000 --no-exit --no-colors -R list --bail",
    "tests-ci": "set -e; mocha --timeout 15000 --no-exit --no-colors -R list --bail ./tests/*-test-parallel.compiled.js; for f in ./tests/*-test.compiled.js ; do npm run test-ci $f; done",
    "benchmark-ci": "set -e; mocha --timeout 1200000 --no-exit --no-colors -R spec --bail",
    "benchmarks-ci": "set -e;for f in ./tests/benchmark/*-test.compiled.js ; do npm run benchmark-ci $f; done",
    "lint": "eslint --ext js,.es6,.jsx ./",
    "lint-ci": "set -e; eslint --no-color --quiet --ext js,.es6,.jsx ./",
    "boss": "export NODE_ENV=staging; set -e; branch-off start local-stage",
    "bos": "set -e; export NODE_ENV=development; branch-off start local-release",
    "bot": "set -e; branch-off trigger test local-stage",
    "bod": "set -e; branch-off trigger dev local-stage",
    "bob": "set -e; branch-off trigger benchmark local-stage",
    "local-cordova": "set -e; branch-off trigger cordova-local",
    "local-benchmark": "set -e; branch-off trigger benchmark local-stage",
    "local-test": "set -e; branch-off trigger dev local-stage",
    "local-tests": "set -e; branch-off trigger test local-stage",
    "local-stage": "export NODE_ENV=staging; set -e; branch-off start local-stage",
    "local-release": "set -e; export NODE_ENV=development; branch-off start local-release",
    "database-drop": "cd ./scripts && ./mysql-drop.sh",
    "database-upgrade": "cd ./scripts && ./mysql-upgrade.sh",
    "production": "grunt production",
    "cordova-emulate": "cd cordova && cordova emulate ios",
    "compile": "grunt compile",
    "validate": "npm ls"
  },
